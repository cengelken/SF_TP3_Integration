<div class="list-inline" >
  <h3>Case # <%= @case.case_num %></h3>
</div>
<div class="row">
  <table class="table">
    <tr>
      <th>Task #</th>
      <th>Next Action</th>
      <th>Owner</th>
      <th>Due Date</th>
    </tr>
    <% @case.tasks.each_with_index do |task_obj, index| %>
      <tr>
        <th><%= link_to index+1, @case.url %></th>
        <th><%= task_obj.next_action %></th>
        <th><%= task_obj.owner %></th>
        <th><%= task_obj.due_date %></th>
      </tr>
    <% end %>
 </table>
</div>
