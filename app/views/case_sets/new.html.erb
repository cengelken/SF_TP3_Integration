<div class="list-inline" >
  <%= button_to 'Cancel', case_set_path(:id => @case_set.id), :method => :delete, class: 'btn btn-warning btn-xs pull-right' %>
  <%= button_to 'Push to TP3', case_set_path(:id => @case_set.id), :method => :post, class: 'btn btn-success btn-xs pull-right', style: 'margin-right: 5px' %>
  <h3>Escalation's Current Cases</h3>
</div>
<div class="row">
  <table class="table">
    <tr>
      <th>Case #</th>
      <th>Description</th>
      <th>Owner</th>
    </tr>
    <% @cases.each do |case_obj| %>
      <tr>
        <th><%= link_to case_obj.case_num, case_obj.url %></th>
        <th><%= case_obj.description %></th>
        <th><%= case_obj.owner %></th>
      </tr>
    <% end %>
 </table>
</div>
